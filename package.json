{
  "name": "gutcheck-score-mvp",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "export": "next build",
    "deploy": "npm run build && firebase deploy",
    "start": "next start",
    "lint": "next lint",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:browserstack": "playwright test --config=browserstack.config.js",
    "test:visual": "playwright test tests/visual-consistency.spec.ts",
    "test:assessment": "playwright test tests/assessment-flow.spec.ts",
    "test:report": "playwright show-report",
    "browserstack:local": "node scripts/start-browserstack-local.js",
    "test:browserstack:local": "npm run browserstack:local & sleep 10 && npm run test:browserstack && pkill -f 'browserstack-local'",
    "test:browserstack:sdk": "npx browserstack-node-sdk playwright test --config=./playwright.config.ts",
    "test:browserstack:simple": "npx browserstack-node-sdk playwright test tests/browserstack-simple.spec.ts --config=./playwright.config.ts",
    "test:browserstack:traditional": "playwright test --config=./playwright.browserstack.config.ts tests/browserstack-traditional.spec.ts",
    "postinstall": "npm update browserstack-node-sdk",
    "browserstack:local-browserstack": "browserstack-node-sdk node scripts/start-browserstack-local.js",
    "test-browserstack": "browserstack-node-sdk playwright test",
    "test:ui-browserstack": "browserstack-node-sdk playwright test --ui",
    "test:headed-browserstack": "browserstack-node-sdk playwright test --headed",
    "test:browserstack-browserstack": "browserstack-node-sdk playwright test --config=browserstack.config.js",
    "test:visual-browserstack": "browserstack-node-sdk playwright test tests/visual-consistency.spec.ts",
    "test:assessment-browserstack": "browserstack-node-sdk playwright test tests/assessment-flow.spec.ts",
    "test:report-browserstack": "browserstack-node-sdk playwright show-report",
    "test:browserstack:traditional-browserstack": "browserstack-node-sdk playwright test --config=./playwright.browserstack.config.ts tests/browserstack-traditional.spec.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "clsx": "^2.1.1",
    "firebase": "^12.0.0",
    "firebase-admin": "^13.4.0",
    "lucide-react": "^0.534.0",
    "next": "15.4.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.61.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "browserstack-local": "^1.5.8",
    "browserstack-node-sdk": "^1.40.6",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}